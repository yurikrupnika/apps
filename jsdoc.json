{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "../"
    ],
    "excludePattern": "(node_modules|docs|dist|ejs)",
    "includePattern": ".(js|jsx)$"
  },
  "sourceType": "module",
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "dist/assets/docs",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "plugins": [
    "plugins/markdown"
  ]
}
