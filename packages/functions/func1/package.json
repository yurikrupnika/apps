{
  "name": "func1",
  "version": "1.0.0",
  "main": "dist/index.js",
  "engines": {
    "node": "10"
  },
  "private": true,
  "files": [
    "dist"
  ],
  "watch": {
    "fucntion": "dist/*.js"
  },
  "config": {
    "port": "3334"
  },
  "scripts": {
    "build:watch": "rollup --config=../../../rollup.config.functions.js -w",
    "watch": "npm-watch fucntion",
    "fucntion": "functions-framework --target=$npm_package_name --port=$npm_package_config_port",
    "start": "run-p build:watch watch",
    "prebuild": "rimraf dist",
    "lint": "eslint src --ext .js,.jsx --color",
    "test": "jest src --colors",
    "build": "rollup --config=../../../rollup.config.functions.js",
    "predeploy": "npm run build",
    "deploy:functions": "gcloud functions deploy $npm_package_name --runtime nodejs$npm_package_engines_node --trigger-http --entry-point=$npm_package_name --source=dist"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.3.2",
    "npm-watch": "^0.6.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@google-cloud/translate": "^5.1.2",
    "@krupnik/button": "^30.1.5",
    "@krupnik/render": "^19.1.1",
    "axios": "^0.19.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "react": "^16.12.0"
  }
}
