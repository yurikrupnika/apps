{
  "name": "func-zero",
  "version": "1.0.0",
  "main": "dist/index.js",
  "engines": {
    "node": "10"
  },
  "private": true,
  "files": [
    "dist"
  ],
  "watch": {
    "fucntion": "dist/*.js"
  },
  "config": {
    "port": "3333"
  },
  "scripts": {
    "build:watch": "rollup --config=../rollup.config.js -w",
    "watch": "npm-watch fucntion",
    "fucntion": "functions-framework --target=$npm_package_name --port=$npm_package_config_port",
    "starts": "run-p build:watch watch",
    "prebuild": "rimraf dist",
    "lint": "eslint src --ext .js,.jsx --color",
    "test": "jest src --colors",
    "build": "rollup --config=../rollup.config.js",
    "predeploy": "npm run build",
    "deploy": "gcloud functions deploy $npm_package_name --runtime nodejs$npm_package_engines_node --trigger-http --entry-point=$npm_package_name"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.3.2",
    "npm-watch": "^0.6.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
