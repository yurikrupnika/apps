{
  "name": "@krupnik/fe-docs",
  "version": "1.0.0",
  "private": true,
  "description": "documintation ",
  "author": "yurikrupnik <krupnik.yuri@gmail.com>",
  "homepage": "https://github.com/yurikrupniktools/client-apps/tree/master/packages/docs#readme",
  "license": "ISC",
  "main": "dist/server.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupniktools/client-apps.git"
  },
  "scripts": {
    "start": "run-p starts starts:swagger start:client start:server",
    "start:server": "webpack --config=../../webpack.config.server.js -w --color",
    "start:client": "webpack-dev-server --config=../../webpack.config.client.js --color",
    "starts": "nodemon --exec 'jsdoc -c ../../jsdoc.json' --watch --verbose",
    "starts:swagger": "nodemon --exec 'swagger-jsdoc -d ../../swagger-jsdoc.js' --watch --verbose",
    "startsssss": "npm run starts & npm starts:swagger & npx http-server -c-1 -p 6002 dist",
    "prebuild": "rimraf dist swagger.json",
    "build:docs": "jsdoc -c ../../jsdoc.json",
    "swagger": "swagger-jsdoc -d ../../swagger-jsdoc.js",
    "build": "run-s swagger build:docs build:client build:server",
    "build:predocs": "",
    "build:client": "webpack --config=../../webpack.config.client.js --env.prod --color",
    "build:server": "webpack --config=../../webpack.config.server.js --env.prod --color",
    "build:coverage": "jest --config=../../ ."
  },
  "bugs": {
    "url": "https://github.com/yurikrupniktools/client-apps/issues"
  },
  "dependencies": {
    "@krupnik/render": "^4.0.3",
    "@material-ui/core": "^4.3.1",
    "axios": "^0.19.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "jsdoc-x": "^4.0.3",
    "koa": "^2.7.0",
    "koa-render-view": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "swagger-koa": "^2.1.0",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@testing-library/react": "^9.0.2"
  },
  "jest": {
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
